{
  "name": "matche",
  "version": "1.0.0",
  "description": "Travel Matchmaking Monorepo",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "turbo run build",
    "predev": "docker-compose up -d",
    "dev": "turbo run dev --parallel",
    "db:push": "turbo run db:push",
    "db:studio": "pnpm --filter @matche/db studio",
    "clean": "rm -rf node_modules && pnpm recursive exec -- rm -rf node_modules",
    "kill": "npx kill-port 3000 3001 5555"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.1",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "turbo": "^2.7.5",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "zeptomatch": "2.1.0"
    },
    "ignoredBuiltDependencies": [
      "@prisma/engines",
      "msgpackr-extract",
      "sharp",
      "unrs-resolver"
    ],
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@prisma/client",
      "prisma",
      "puppeteer"
    ]
  }
}